from timeit import default_timer as timer

start = timer()

num 

lijst = [int(x) for x in str(num)]

max = 1000
for i in range(0, len(lijst)-13):
    value = lijst[i]*lijst[i+1]*lijst[i+2]*lijst[i+3]*lijst[i+4]*lijst[i+5]*lijst[i+6]*lijst[i+7]*lijst[i+8]*lijst[i+9]*lijst[i+10]*lijst[i+11]*lijst[i+12]
    if value > max:
        max = value
    else:
        continue
end = timer()

print(end-start)
print(max)
